<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>DAVNOR MEDICON</title>
    <link rel="icon" href="~/Content/ISPINIA/img/MedconICon-01.ico" type="image/x-icon" />
    <link href="~/Scripts/js/dataTables/datatables.min.css" rel="stylesheet" />
    <link href="~/Scripts/js/select2/select2.min.css" rel="stylesheet" />
    <link href="~/Content/ISPINIA/awesome-bootstrap-checkbox/awesome-bootstrap-checkbox.css" rel="stylesheet" />
    <link href="~/Content/ISPINIA/sweetalert/sweetalert.css" rel="stylesheet" />

    <link href="~/Content/ISPINIA/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/ISPINIA/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/ISPINIA/css/animate.css" rel="stylesheet" />
    <link href="~/Content/ISPINIA/css/style.css" rel="stylesheet" />
    @*@Styles.Render("~/Content/css")*@

    <style>
         .center-content {
        display: flex;
        justify-content: center;
        align-items: center;
        height: 100%;
    }
    </style>
    
</head>
<body>
    <div id="Spinner" style="background-color: #2f4050; width: 100vw; height: 100%; position:fixed; top: 0; left: 0; z-index: 10000;">
        <div class="center-content">
            <div class="lds-ripple"><div></div><div></div></div>
        </div>
    </div>

    

    <div id="wrapper">
        <nav class="navbar-default navbar-static-side" role="navigation">
            <div class="sidebar-collapse">
                <ul class="nav metismenu" id="side-menu">
                    <li class="nav-header">
                        <div class="dropdown profile-element">
                            <span>
                                <img alt="image" class="img-circle" src="~/Content/ISPINIA/img/profile_64.png" />
                              @*<img alt="image" class="img-circle" src="~/Content/img/profile_small.jpg" />*@
                            </span>
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                                <span class="clear">
                                    <span class="block m-t-xs">
                                        <strong class="font-bold">@Session["firstName"]  @Session["middleName"] @Session["lastName"] @Session["extName"]</strong>
                                    </span> <span class="text-muted text-xs block" style="font-weight:600;">@Session["userTypeDesc"]</span>
                                </span>
                            </a>
                        </div>
                        <div class="logo-element">
                            <i class="fa fa-user-md"></i>
                            <small style="font-size:12px;">MEDICON</small>
                                <br />
                        </div>
                    </li>
                    <li class="active">
                        <a href="~/Home/Index"><i class="fa fa-th-large"></i> <span class="nav-label">Dashboards</span></a>
                    </li>
                    <li id="ANALYTICS">
                        <a><i class="fa fa fa-bar-chart-o"></i> <span class="nav-label">Analytics</span><span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level collapse" id="ANALYTICSsub"></ul>
                    </li>
                    <li id="VITALSIGNS">
                        <a href="../VitalSigns/VSview"><i class="fa fa-stethoscope"></i> <span class="nav-label">Vital Signs</span></a>
                    </li>
                    <li id="MEDCON">
                        <a href="../MedicalConsultation/Index"><i class="fa fa-user-md"></i> <span class="nav-label">Medical Consultation</span></a>
                    </li>
                    <li id="GLASSES">
                        <a href="../ReadingGlasses/Index"><i class="fa fa-eye"></i> <span class="nav-label">Reading Glasses</span></a>
                    </li>
                    <li id="LAB">
                        <a><i class="fa fa-eyedropper"></i> <span class="nav-label">Laboratory</span><span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level collapse" id="LABsub">
                        </ul>
                    </li>
                    <li id="PBE">
                        <a href="../PapsmearBreastExam/Index"><i class="fa fa-female"></i> <span class="nav-label">Papsmear & Breast</span></a>
                    </li>
                    <li id="MRH">
                        <a><i class="fa fa-male"></i> <span class="nav-label">Male Reproductive</span><span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level collapse" id="MRHsub">
                        </ul>
                    </li>
                    <li id="HEALTH">
                        <a><i class="fa fa-stethoscope"></i> <span class="nav-label">Dental</span><span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level collapse" id="HEALTHsub">
                        </ul>
                    </li>
                    <li id="XRAY">
                        <a><i class="fa fa-exclamation-triangle"></i> <span class="nav-label">X-ray</span><span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level collapse" id="XraySub"></ul>
                    </li>
                    <li id="PHARM">
                        <a href="../Pharmacy/Dispensing"><i class="fa fa-female"></i> <span class="nav-label">Pharmacy</span></a>
                    </li>
                    <li id="SCHEDULER">
                        <a><i class="fa fa-calendar"></i> <span class="nav-label">Scheduler</span><span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level collapse" id="schedulerSub"></ul>
                    </li>
                    <li id="REPORTS">
                        <a><i class="fa fa-signal"></i> <span class="nav-label">Reports</span><span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level collapse" id="REPORTSsub"></ul>
                    </li>
                    <li id="UTILITY">
                        <a><i class="fa fa-gear"></i> <span class="nav-label">Utility</span><span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level collapse" id="UTILITYsub">
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>

        <div id="page-wrapper" class="gray-bg dashbard-1">
            <div class="row border-bottom">
                <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0">
                    <div class="navbar-header">
                        <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i> </a>
                    </div>
                    <ul class="nav navbar-top-links navbar-right">
                        <li>
                            <span class="m-r-sm text-muted welcome-message">Welcome to MEDICON</span>
                        </li>
                     
                        <li>
                            <a href="~/Account/Signout">
                                <i class="fa fa-sign-out"></i> Log out
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>

            <div class="body-content" ng-app="MedConApp">
                @RenderBody()

                <div class="footer">
                    <div class="pull-right">
                        Template by <strong>INSPINIA</strong>
                    </div>
                    <div>
                        <strong style="color:#66b2b2;"> DAVNOR MEDICON &copy; 2022</strong>
                    </div>
                </div>
            </div>

        </div>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
  
    @RenderSection("scripts", required: false)

    <script src="~/Scripts/js/QRScan/instascan.min.js"></script>
    <script src="~/Scripts/js/dataTables/datatables.min.js"></script>
    <script src="~/Scripts/js/select2/select2.full.min.js"></script>
    <script src="~/Content/ISPINIA/sweetalert/sweetalert.min.js"></script>
    <script src="~/Scripts/js/Moment/moment.min.js"></script>
    <script src="~/Scripts/js/Moment/moment-timezone-with-data.min.js"></script>
 
    <script>
        $(document).ready(function () {
            $('#Spinner').hide();
        });
    </script>



    <script>
        $(function(){

            var utility = $('#UTILITY');
            var health = $('#HEALTH');
            var vitalSigns = $('#VITALSIGNS'); 
            var medcon = $('#MEDCON');
            var glasses = $('#GLASSES');
            var lab = $('#LAB');
            var pbe = $('#PBE');
            var mrh = $('#MRH');
            var pharm = $('#PHARM');
            var analytics = $('#ANALYTICS');
            var xray = $('#XRAY');
            var scheduler = $('#SCHEDULER');
            var reports = $('#REPORTS');

            health.hide();
            utility.hide();
            vitalSigns.hide();
            medcon.hide();
            glasses.hide();
            lab.hide();
            pbe.hide();
            mrh.hide();
            pharm.hide();
            analytics.hide();
            xray.hide();
            scheduler.hide();
            reports.hide();

            $.post('../Home/gMenuAccess').then(function (d) {
                for(var x =0;x<d.length;x++){
                    if(d[x].mainMenu =='UTILITY'){
                        utility.show();
                        $('#UTILITYsub').append('<li class="nav-item"><a href="../'+d[x].controller+'/'+d[x].action+'" class="nav-link"> '+d[x].menuDesc+'</a></li>');
                    }

                    else  if (d[x].mainMenu == 'HEALTH') {
                        health.show();
                        $('#HEALTHsub').append('<li class="nav-item"><a href="../' + d[x].controller + '/' + d[x].action + '" class="nav-link"> ' + d[x].menuDesc + '</a></li>');
                    }

                    else if (d[x].mainMenu == 'VITAL SIGNS') {
                        vitalSigns.show();
                    }

                    else if (d[x].mainMenu == 'MEDCON') {
                        medcon.show();
                    }

                    else if (d[x].mainMenu == 'READING GLASSES') {
                        glasses.show();
                    }

                    else if (d[x].mainMenu == 'LAB') { 
                        lab.show();
                        $('#LABsub').append('<li class="nav-item"><a href="../' + d[x].controller + '/' + d[x].action + '" class="nav-link"> ' + d[x].menuDesc + '</a></li>');
                    }

                    else if (d[x].mainMenu == 'PBE') {
                        pbe.show();
                    }

                    else if(d[x].mainMenu =='MRH'){
                        mrh.show();
                        $('#MRHsub').append('<li class="nav-item"><a href="../' + d[x].controller + '/' + d[x].action + '" class="nav-link"> ' + d[x].menuDesc + '</a></li>');
                    }

                    else if (d[x].mainMenu == 'XRAY') {
                        xray.show();
                        $('#XraySub').append('<li class="nav-item"><a href="../' + d[x].controller + '/' + d[x].action + '" class="nav-link"> ' + d[x].menuDesc + '</a></li>');
                    }

                    else if (d[x].mainMenu == 'PHARM') {
                        pharm.show();
                    }

                    else if (d[x].mainMenu == 'SCHEDULER') {
                        scheduler.show();
                        $('#schedulerSub').append('<li class="nav-item"><a href="../' + d[x].controller + '/' + d[x].action + '" class="nav-link"> ' + d[x].menuDesc + '</a></li>');
                    }

                    else if (d[x].mainMenu == 'ANALYTICS') {
                        analytics.show();
                        $('#ANALYTICSsub').append('<li class="nav-item"><a href="../' + d[x].controller + '/' + d[x].action + '" class="nav-link"> ' + d[x].menuDesc + '</a></li>');
                    }

                    else if (d[x].mainMenu == 'REPORTS') {
                        reports.show();
                        $('#REPORTSsub').append('<li class="nav-item"><a href="../' + d[x].controller + '/' + d[x].action + '" class="nav-link"> ' + d[x].menuDesc + '</a></li>');
                    }
                }
            })
        })
</script>


</body>
</html>
